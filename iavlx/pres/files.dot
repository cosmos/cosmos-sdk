digraph G {
    layout=osage
    leaves [shape=box label="leaves.dat" style=filled color=lightgrey]
    branches [shape=box label="branches.dat" style=filled color=lightgrey]
    subgraph cluster_KVLog {
        label=<kv.log<br/><i>WAL and KV data storage</i>>
        color=lightgrey
